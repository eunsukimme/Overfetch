{
  "name": "overfetch",
  "version": "1.0.0",
  "main": "./server/server.js",
  "repository": "https://github.com/eunsukimme/Overfetch",
  "author": "eunsukimme <eunsu.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.0"
  },
  "scripts": {
    "heroku-postbuild": "cd client && yarn && yarn build",
    "server": "cd server && node server.js",
    "client": "cd client && yarn start",
    "start": "concurrently -k \"yarn server\" \"yarn client\"",
    "dev": "concurrently -k \"cd server && nodemon server\" \"yarn client\""
  },
  "keywords": [
    "example",
    "heroku"
  ],
  "engines": {
    "node": "10.x"
  }
}
